DROP TABLE entity_annual_report_shareholder_equity_change_details_v2;
CREATE TABLE `entity_annual_report_shareholder_equity_change_details_v2` (
  `id` bigint(20) unsigned NOT NULL AUTO_INCREMENT COMMENT '主键',
  `annual_report_year` varchar(50) DEFAULT NULL COMMENT '年报年份',
  `tyc_unique_entity_id` varchar(50) NOT NULL DEFAULT '' COMMENT '实体id',
  `entity_name_valid` varchar(255) NOT NULL DEFAULT '' COMMENT '实体name',
  `entity_type_id` tinyint(4) NOT NULL DEFAULT '1' COMMENT '实体类型',
  `annual_report_tyc_unique_entity_id_shareholder` varchar(50) NOT NULL DEFAULT '' COMMENT '股东实体id',
  `annual_report_entity_name_valid_shareholder` varchar(255) NOT NULL DEFAULT '' COMMENT '股东实体name',
  `annual_report_entity_name_register_shareholder` varchar(255) NOT NULL DEFAULT '' COMMENT '股东实体name快照',
  `annual_report_entity_type_id_shareholder` tinyint(4) NOT NULL DEFAULT '2' COMMENT '股东实体类型',
  `annual_report_equity_ratio_before_change` decimal(38,12) NOT NULL DEFAULT '0.000000000000' COMMENT '变更前股权比例',
  `annual_report_equity_ratio_after_change` decimal(38,12) NOT NULL DEFAULT '0.000000000000' COMMENT '变更后股权比例',
  `annual_report_equity_ratio_change_time` datetime DEFAULT NULL COMMENT '股权变更日期',
  `is_display_annual_report_equity_ratio_change` tinyint(4) NOT NULL DEFAULT '1' COMMENT '是否公示',
  `delete_status` tinyint(4) NOT NULL DEFAULT '0' COMMENT '删除状态',
  `create_time` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
  PRIMARY KEY (`id`),
  KEY `idx_company_year` (`tyc_unique_entity_id`,`annual_report_tyc_unique_entity_id_shareholder`),
  KEY `idx_company_shareholder` (`tyc_unique_entity_id`,`annual_report_tyc_unique_entity_id_shareholder`),
  KEY `idx_shareholder` (`annual_report_tyc_unique_entity_id_shareholder`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COMMENT='年报公示的股东股权变更表';

DROP TABLE entity_annual_report_shareholder_equity_details_v2;
CREATE TABLE `entity_annual_report_shareholder_equity_details_v2` (
  `id` bigint(20) unsigned NOT NULL AUTO_INCREMENT COMMENT '主键',
  `business_id` bigint(20) unsigned NOT NULL DEFAULT '1' COMMENT '业务表(report_shareholder表)主键',
  `annual_report_year` varchar(50) DEFAULT NULL COMMENT '年报年份',
  `tyc_unique_entity_id` varchar(50) NOT NULL DEFAULT '' COMMENT '实体id',
  `entity_name_valid` varchar(255) NOT NULL DEFAULT '' COMMENT '实体name',
  `entity_type_id` tinyint(4) NOT NULL DEFAULT '1' COMMENT '实体类型',
  `annual_report_tyc_unique_entity_id_shareholder` varchar(50) NOT NULL DEFAULT '' COMMENT '股东实体id',
  `annual_report_entity_name_valid_shareholder` varchar(255) NOT NULL DEFAULT '' COMMENT '股东实体name',
  `annual_report_entity_name_register_shareholder` varchar(255) NOT NULL DEFAULT '' COMMENT '股东实体name快照',
  `annual_report_entity_type_id_shareholder` tinyint(4) NOT NULL DEFAULT '2' COMMENT '股东实体类型',
  `annual_report_shareholder_capital_type` varchar(50) NOT NULL DEFAULT '认缴' COMMENT '出资是认缴 or 实缴',
  `annual_report_shareholder_capital_source` varchar(255) DEFAULT NULL COMMENT '业务表原值',
  `annual_report_shareholder_equity_amt` decimal(38,12) NOT NULL DEFAULT '0.000000000000' COMMENT '股东出资额',
  `annual_report_shareholder_equity_currency` varchar(50) NOT NULL DEFAULT '' COMMENT '股东出资额单位及币种',
  `annual_report_shareholder_equity_valid_date` datetime DEFAULT NULL COMMENT '出资日期',
  `annual_report_shareholder_equity_submission_method` varchar(50) NOT NULL DEFAULT '' COMMENT '出资方式',
  `is_display_annual_report_shareholder_equity_details` tinyint(4) NOT NULL DEFAULT '1' COMMENT '是否公示',
  `delete_status` tinyint(4) NOT NULL DEFAULT '0' COMMENT '删除状态',
  `create_time` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
  PRIMARY KEY (`id`),
  KEY `idx_business_id` (`business_id`),
  KEY `idx_company_year` (`tyc_unique_entity_id`,`annual_report_year`),
  KEY `idx_company_shareholder` (`tyc_unique_entity_id`,`annual_report_tyc_unique_entity_id_shareholder`),
  KEY `idx_shareholder` (`annual_report_tyc_unique_entity_id_shareholder`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COMMENT='年报股东及出资信息明细表';


DROP TABLE entity_annual_report_investment_details_v2;
CREATE TABLE `entity_annual_report_investment_details_v2` (
  `id` bigint(20) unsigned NOT NULL AUTO_INCREMENT COMMENT '主键',
  `annual_report_year` varchar(50) DEFAULT NULL COMMENT '年报年份',
  `tyc_unique_entity_id` varchar(50) NOT NULL DEFAULT '' COMMENT '实体id',
  `entity_name_valid` varchar(255) NOT NULL DEFAULT '' COMMENT '实体name',
  `entity_type_id` tinyint(4) NOT NULL DEFAULT '1' COMMENT '实体类型',
  `annual_report_company_id_invested` varchar(50) NOT NULL DEFAULT '' COMMENT '被投资实体id',
  `annual_report_company_name_invested` varchar(255) NOT NULL DEFAULT '' COMMENT '被投资实体name',
  `annual_report_company_name_invested_register_name` varchar(255) NOT NULL DEFAULT '' COMMENT '被投资实体name快照',
  `delete_status` tinyint(4) NOT NULL DEFAULT '0' COMMENT '删除状态',
  `create_time` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
  PRIMARY KEY (`id`),
  KEY `idx_company_year` (`tyc_unique_entity_id`,`annual_report_year`),
  KEY `idx_company_invested_company` (`tyc_unique_entity_id`,`annual_report_company_id_invested`),
  KEY `idx_invested_company` (`annual_report_company_id_invested`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COMMENT='年报对外投资明细表';

DROP TABLE entity_annual_report_ebusiness_details_v2;
