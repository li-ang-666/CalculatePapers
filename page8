  `annual_report_is_ebusiness` tinyint(4) NOT NULL DEFAULT '1' COMMENT '年报登记是否有网上运营',
  `annual_report_ebusiness_type` varchar(50) NOT NULL DEFAULT '' COMMENT '年报登记的网上运营类型',
  `annual_report_ebusiness_name` varchar(555) NOT NULL DEFAULT '' COMMENT '年报登记的网上运营产品名称',
  `annual_report_ebusiness_website` varchar(555) NOT NULL DEFAULT '' COMMENT '年报登记的网址',
  `delete_status` tinyint(4) NOT NULL DEFAULT '0' COMMENT '删除状态',
  `create_time` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
  PRIMARY KEY (`id`),
  KEY `idx_company_year` (`tyc_unique_entity_id`,`annual_report_year`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COMMENT='年报网店运营明细表';




